{
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"version": 3,
	"title": "Minimal Mono",
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#ffffff",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#000000",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#666666",
					"name": "Gray",
					"slug": "gray"
				},
				{
					"color": "#e5e5e5",
					"name": "Light Gray",
					"slug": "light-gray"
				},
				{
					"color": "#f8f8f8",
					"name": "Gray 50",
					"slug": "gray-50"
				},
				{
					"color": "#f0f0f0",
					"name": "Gray 100",
					"slug": "gray-100"
				},
				{
					"color": "#e0e0e0",
					"name": "Gray 200",
					"slug": "gray-200"
				},
				{
					"color": "#cccccc",
					"name": "Gray 300",
					"slug": "gray-300"
				},
				{
					"color": "#999999",
					"name": "Gray 400",
					"slug": "gray-400"
				},
				{
					"color": "#666666",
					"name": "Gray 500",
					"slug": "gray-500"
				},
				{
					"color": "#4d4d4d",
					"name": "Gray 600",
					"slug": "gray-600"
				},
				{
					"color": "#333333",
					"name": "Gray 700",
					"slug": "gray-700"
				},
				{
					"color": "#1a1a1a",
					"name": "Gray 800",
					"slug": "gray-800"
				},
				{
					"color": "#000000",
					"name": "Gray 900",
					"slug": "gray-900"
				},
				{
					"color": "#000000",
					"name": "Accent Black",
					"slug": "giovanni-blue"
				},
				{
					"color": "#000000",
					"name": "Reading Text",
					"slug": "reading-text"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--gray-600)"
					}
				}
			}
		},
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--foreground)",
					"text": "var(--wp--preset--color--background)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--gray-800)",
						"text": "var(--wp--preset--color--background)"
					}
				}
			},
			"core/quote": {
				"border": {
					"left": {
						"color": "var(--wp--preset--color--foreground)"
					}
				}
			}
		}
	}
}