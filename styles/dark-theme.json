						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--gray-700)"
							}
						}
					}
				}
			},
			"core/code": {
				"color": {
					"background": "var(--wp--preset--color--gray-100)",
					"text": "var(--wp--preset--color--gray-900)"
				}
			},
			"core/preformatted": {
				"color": {
					"background": "var(--wp--preset--color--gray-50)",
					"text": "var(--wp--preset--color--gray-900)"
				}
			},
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--radical-red)",
					"text": "var(--wp--preset--color--background)"
				},
				"border": {
					"radius": "8px"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--red-light)",
						"text": "var(--wp--preset--color--background)"
					},
					"border": {
						"radius": "8px"
					}
				}
			},
			"core/quote": {
				"color": {
					"text": "var(--wp--preset--color--gray-700)",
					"background": "var(--wp--preset--color--light-gray)"
				},
				"border": {
					"left": {
						"color": "var(--wp--preset--color--radical-red)",
						"width": "4px"
					},
					"radius": "0 8px 8px 0"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--4)",
						"right": "var(--wp--preset--spacing--6)",
						"bottom": "var(--wp--preset--spacing--4)",
						"left": "var(--wp--preset--spacing--6)"
					}
				}
			},
			"core/pullquote": {
				"color": {
					"text": "var(--wp--preset--color--foreground)",
					"background": "var(--wp--preset--color--light-gray)"
				},
				"border": {
					"color": "var(--wp--preset--color--radical-red)",
					"width": "2px",
					"radius": "12px"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--8)",
						"right": "var(--wp--preset--spacing--8)",
						"bottom": "var(--wp--preset--spacing--8)",
						"left": "var(--wp--preset--spacing--8)"
					}
				}
			},
			"core/post-date": {
				"color": {
					"text": "var(--wp--preset--color--gray-500)"
				}
			},
			"core/post-author": {
				"color": {
					"text": "var(--wp--preset--color--gray-500)"
				}
			},
			"core/post-terms": {
				"color": {
					"text": "var(--wp--preset--color--gray-500)"
				}
			}
