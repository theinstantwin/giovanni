	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--reading-text)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--giovanni-blue)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--gray-700)"
					}
				}
			}
		},
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--giovanni-blue)",
					"text": "var(--wp--preset--color--background)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--gray-700)",
						"text": "var(--wp--preset--color--background)"
					}
				}
			},
			"core/quote": {
				"color": {
					"text": "var(--wp--preset--color--gray-700)"
				},
				"border": {
					"left": {
						"color": "var(--wp--preset--color--giovanni-blue)"
					}
				}
			},
			"core/pullquote": {
				"color": {
					"text": "var(--wp--preset--color--gray-800)",
					"background": "var(--wp--preset--color--gray-50)"
				}
			}
		}
	}
