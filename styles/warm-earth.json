{
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"version": 3,
	"title": "Warm Earth",
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#fdf9f4",
					"name": "Background",
					"slug": "background"
				},
				{
					"color": "#faf7f0",
					"name": "Container",
					"slug": "container"
				},
				{
					"color": "#2d1810",
					"name": "Foreground",
					"slug": "foreground"
				},
				{
					"color": "#8b6914",
					"name": "Warm Brown",
					"slug": "gray"
				},
				{
					"color": "#f4e6d1",
					"name": "Light Beige",
					"slug": "light-gray"
				},
				{
					"color": "#d2691e",
					"name": "Earth Orange",
					"slug": "giovanni-blue"
				},
				{
					"color": "#d2691e",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#2d1810",
					"name": "Reading Text",
					"slug": "reading-text"
				},
				{
					"color": "#8fbc8f",
					"name": "Forest Green",
					"slug": "success"
				},
				{
					"color": "#daa520",
					"name": "Goldenrod",
					"slug": "warning"
				},
				{
					"color": "#cd853f",
					"name": "Peru",
					"slug": "error"
				},
				{
					"color": "#4682b4",
					"name": "Steel Blue",
					"slug": "info"
				}
			]
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--giovanni-blue)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--foreground)"
					}
				}
			}
		},
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--giovanni-blue)",
					"text": "var(--wp--preset--color--background)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--foreground)",
						"text": "var(--wp--preset--color--background)"
					}
				}
			},
			"core/quote": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"border": {
					"left": {
						"color": "var(--wp--preset--color--giovanni-blue)"
					}
				}
			},
			"core/pullquote": {
				"color": {
					"text": "var(--wp--preset--color--foreground)",
					"background": "var(--wp--preset--color--light-gray)"
				}
			}
		}
	}
}